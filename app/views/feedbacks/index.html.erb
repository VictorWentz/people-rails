<section>
  <table>
    <thead>
    <tr>
      <th>Nome</th>
      <th>Função</th>
      <th>Departamento</th>
      <th>Ultimos feedbacks</th>
      <th>Novo Feedback</th>
      <th>Quantidade</th>
    </tr>
    </thead>
    <tbody>
    <% @usersFeedback.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td>"Placeholder"</td>
        <td><%= user.departaments.first.name%></td>
        <td><%=button_to "feedbacks lista", user_feedback_list_path(user_id: user.id), method: :get  %></td>
        <td><%=link_to "novo feedback", new_feedback_path(user_id: user.id) %></td>
        <td><%=user.feedbacks.size %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</section>