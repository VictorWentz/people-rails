<section class = "main-login-page">
  <section class = "image-login">
    <img src='https://placehold.co/750x750'>
  </section>
  <section class = "main-login">
    <div class="main-login-content">
      <h1>
        Login
      </h1>
      <%=form_with(url: session_path) do |f| %>
        <%= f.label :email %>
        <%= f.email_field :email, autofocus: true %>

        <%= f.submit "Enter" %>
      <% end %>
    </div>
  </section>
</section>